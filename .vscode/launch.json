    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter Launch Chrome",
            "cwd": "frontend",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "deviceId": "chrome",
            "args": [
                "--web-port",
                "50547"
            ]
        },
        {
            "name": "flutter Launch Android",
            "cwd": "frontend",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "deviceId": "a13f568f",
            "args": [
                "--dart-define",
                "BASE_URL=https://storystains.fly.dev/api"
            ]
        },
        {
            "name": "frontend macos",
            "cwd": "frontend",
            "request": "launch",
            "deviceId": "macos",
            "type": "dart"
        },
        {
            "name": "frontend",
            "cwd": "frontend",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "frontend (profile mode)",
            "cwd": "frontend",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "frontend (release mode)",
            "cwd": "frontend",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}